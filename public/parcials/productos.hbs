

<table class="table table-dark table-striped  ">
    <thead>
        <tr>            
            <th>Producto</th>
            <th>Precio</th>
            <th>Imagen</th>
            
        </tr>

        <tbody>
         {{#each productos}}
             <tr>                 
                 <th>{{this.name}}</th>
                 <th>{{this.price}}</th>
                 <th><img class="img-thumbnail" src={{this.picture}} alt={{this.id}}></th>
             </tr>
         {{/each}}
        </tbody>
    </thead>

</table>
